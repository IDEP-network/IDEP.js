{
  "name": "idep.js",
  "version": "0.0.3-beta.1",
  "description": "IDEP javascript SDK",
  "author": "radekxrandom",
  "repository": {
    "type": "git",
    "url": "https://github.com/IDEP-network/IDEP.js.git"
  },
  "private": false,
  "license": "Apache-2.0",
  "main": "dist/node/index.cjs.development.js",
  "browser": "dist/browser/index.js",
  "typings": "dist/types",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build.js && mkdir dist/node/proto-types && mkdir dist/browser/proto-types && cp -r src/types/proto-types/* dist/browser/proto-types && cp -r src/types/proto-types/* dist/node/proto-types && cp -r src/bip39/wordlists dist/browser && cp -r src/bip39/wordlists dist/node",
    "build:browser": "ENV_TYPE=browser tsdx build --target web --format esm --name index",
    "build:node": "ENV_TYPE=node tsdx build --target node --format cjs --name index",
    "test": "tsdx test",
    "test-cov": "tsdx test  --coverage"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": ">=16.13.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.11",
    "@types/ripemd160": "^2.0.0",
    "@types/secp256k1": "^4.0.1",
    "@types/sha.js": "^2.4.0",
    "@types/wif": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup": "^2.64.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bech32": "^2.0.0",
    "bn.js": "^5.2.0",
    "buffer": "^6.0.3",
    "conf": "^10.1.1",
    "decimal.js": "^10.2.1",
    "dotenv": "^10.0.0",
    "elliptic": "^6.5.4",
    "google-protobuf": "^3.19.1",
    "grpc": "^1.24.11",
    "grpc-web": "^1.3.0",
    "is-buffer": "^2.0.5",
    "js-sha3": "^0.8.0",
    "long": "^5.2.0",
    "ripemd160": "^2.0.2",
    "scrypt-pbkdf": "^1.0.7",
    "secp256k1": "^4.0.2",
    "sha.js": "^2.4.11",
    "stream-browserify": "^3.0.0",
    "typeforce": "^1.18.0"
  }
}
