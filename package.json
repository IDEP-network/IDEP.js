{
  "name": "IDEPjs",
  "version": "0.0.1",
  "description": "IDEP javascript SDK",
  "author": "radekxrandom",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "14.17.5"
  },
  "module": "dist/idepright.esm.js",
  "size-limit": [
    {
      "path": "dist/idepright.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/idepright.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/crypto-js": "^4.0.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.10.2",
    "@types/readable-stream": "^2.3.9",
    "@types/secp256k1": "^4.0.1",
    "@types/tmp": "^0.2.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "buffer": "^6.0.3",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@tendermint/belt": "^0.3.0",
    "@types/ripemd160": "^2.0.0",
    "ava": "^3.15.0",
    "axios": "^0.21.1",
    "bech32": "^2.0.0",
    "bip32": "^2.0.6",
    "bip39": "^3.0.3",
    "bufferutil": "^4.0.3",
    "crypto-js": "^4.1.1",
    "decimal.js": "^10.2.1",
    "js-sha256": "^0.9.0",
    "jscrypto": "^1.0.1",
    "readable-stream": "^3.6.0",
    "ripemd160-js": "^1.2.0",
    "secp256k1": "^4.0.2",
    "sha256": "^0.2.0",
    "tmp": "^0.2.1",
    "utf-8-validate": "^5.0.5",
    "ws": "^7.4.2"
  }
}
